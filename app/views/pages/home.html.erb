<div class="navbar-home">
  <%= link_to root_path, class: "navbar-wagon-brand" do %>
  <%= image_tag "logo_white.png" %>
  <% end %>
  <!-- Right Navigation -->
  <div class="navbar-home-right hidden-xs hidden-sm">
  <% if !user_signed_in? %>

   <%= link_to "List your Venue", new_user_registration_path, class: "btn-navbar"%>

    <%= link_to "Sign Up", new_user_registration_path, class: "btn-navbar"%>

    <%= link_to "Login", new_user_session_path, class: "btn-navbar"%>
      <% else %>

      <%= link_to "List Your Venue", new_venue_path, class: "btn-red"%>

      <% if current_user.profile %>

      <%= link_to "Go to Profile", profile_path, class: "btn-navbar" %>
      <% else %>
      <%= link_to "Create Profile", new_profile_path, class: "btn-navbar" %>
     <% end %>


      <%= link_to destroy_user_session_path, method: :delete do %>

        <p class="btn-navbar", style="text-decoration: none"> <%= t(".sign_out", default: "Log out") %></p>
      <% end %>
    <% end %>
  </div>
</div>


<div class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url('http://www.homejournal.hk/wp-content/uploads/2016/03/Issaya-Bar.jpg');">

  <div class="banner-content">
    <h1>Find the perfect location for your</h1>
    <h1 class="typewrite" data-period="2000" data-type='[ "wedding.", "dinner.", "meeting.", "photoshoot.", "workshop.", "birthday." ]'>
      <span class="wrap"></span>
    </h1>
    <br>
    <div class="home-form" >
      <form action="#">
        <input type="text" name="Location" placeholder="City, Country" style = "border: 1px grey; padding: 7px 20px;">
        <input type="text" name="Category" placeholder ="What are you planning?" style = "border: 1px grey; padding: 7px 20px;">
        <input type="submit" value="Search" style="background-color: #DE1A1A; border: 1px #DE1A1A; color: #CECECE; padding: 7px 7px; width: 100px;">
      </form>
    </div>
    <br>
    <br>
    <br>
    <br>
  </div>
</div>

