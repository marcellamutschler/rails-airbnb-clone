<div class="text-center">
  <h1>Book this place</h1>
</div>

<div class="container">
  <div class="row">
    <div class="col-sm-6 col-sm-offset-3">

      <%=  simple_form_for([@venue, @booking]) do |f| %>
      <%= f.input :total_price%>

      <%= f.submit 'Book Venue!', class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
