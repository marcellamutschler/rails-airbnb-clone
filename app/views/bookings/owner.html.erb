 <h1>All your bookings </h1>


<% if current_user.venues.present? %>
<h1>This is all the bookings on your flats</h1>

<% current_user.venues.each do |venue| %>
<ul>
  <li>
  <%= venue.name %> - <%= venue.location %>
   </li>
   <% venue.bookings.each do |booking| %>
   <% raise %>

   <p> booked by <%= booking.user.email %> for <%= booking.hours %> hours on <%= booking.start_date %></p>
   <%= booking.status %>, <%= link_to "Accept", accept_profile_booking_path(@profile, booking) %> <%= link_to "Decline", decline_profile_booking_path(@profile, booking) %>
   <%= booking.total_price %> €
    <ul>

     <li>  </li>
    </ul>
    <% end %>
 </ul>
<% end %>
<% end %>



