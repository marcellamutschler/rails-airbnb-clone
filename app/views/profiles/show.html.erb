<div class="container">

  <h1>This is your profile</h1>
  <% avatar_url = current_user.facebook_picture_url || "http://placehold.it/30x30" %>
<%= image_tag avatar_url %>
  <br>
  <h2>Your name:
  <%= @profile.first_name %>
  <%= @profile.last_name %>
  </h2>
  <br>
  <h3>Your description:
  <%= @profile.bio %>
  </h3>

  <%= link_to "Edit Profile", edit_profile_path, class: "btn btn-primary"%>



</div>
